   <children>
      <HBox prefHeight="321.0" prefWidth="348.0">
         <children>
            <TextField fx:id="heightText" prefHeight="26.0" prefWidth="93.0" promptText="height" />
            <TextField fx:id="widthText" prefHeight="26.0" prefWidth="105.0" promptText="width" />
            <Button fx:id="createButton" mnemonicParsing="false" onMouseClicked="#initialize" prefHeight="26.0" prefWidth="71.0" text="Create" />
         </children>
      </HBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
