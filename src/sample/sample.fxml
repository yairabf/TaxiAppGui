<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" prefHeight="202.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
         <top>
            <HBox fx:id="hBox" alignment="CENTER" prefHeight="0.0" prefWidth="500.0" BorderPane.alignment="TOP_CENTER">
               <children>
                  <TextField fx:id="heightText" prefHeight="26.0" prefWidth="93.0" promptText="height" />
                  <TextField fx:id="widthText" prefHeight="26.0" prefWidth="105.0" promptText="width" />
                  <Button fx:id="createButton" mnemonicParsing="false" onMouseClicked="#initialize" prefHeight="26.0" prefWidth="71.0" text="Create" />
               </children>
            </HBox>
         </top>
   <center>
      <VBox fx:id="center" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </center>
      </BorderPane>
